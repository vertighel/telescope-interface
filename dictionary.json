{

    
    "dome": {

	"status" : {
	    "type" : "bool",
	    "range" : [true, false],
	    "default" : false,
	    "description" : "Dome control system status, on (true) or off (false)"
	},

	"shutter" : {
	    "type" : "bool",
	    "range" : [true, false],
	    "default" : false,
	    "description" : "Shutter status, open (true) or closed (false)."
	},

	"follow" : {
	    "type" : "bool",
	    "range" : [true, false],
	    "default" : false,
	    "description" : "Dome linked to the telescope position, (true) or not (false)."
	},

	"position" : {
	    "type" : "integer",
	    "range" : [0,359],
	    "default" : 0,
	    "description" : "Shutter angle in degrees, clockwise from North. Instruction is read if .follow is false. "
	},

	"description" : "Dome description."
    },


    "telescope" : {

	"status" : {
	    "type" : "bool",
	    "range" : [true, false],
	    "default" : false,
	    "description" : "Telescope status, on (true) or off (false)."
	},

	"petals" : {
	    "type" : "bool",
	    "range" : [true, false],
	    "default" : false,
	    "description" : "Primary petals, open (true) or closed (false)."
	},

	"coordinates" : {
	    "type" : "string",
	    "range" : ["alt-az","radec"],
	    "default" : "alt-az",
	    "description" : "Coordinates system: alt-az, radec. If \"alt-az\" is set, then .alt and .az instructions are read.  If \"radec\" is set, then .ra and .dec instructions are read. "
	},

	"ra" : {
	    "type" : "sexagesimal",
	    "range" : ["00 00 00.0", "23 59 59.9"],
	    "default" : "00 00 00.0",
	    "description" : "Sexagesimal right ascension α."
	},

	"dec" :  {
	    "type" : "sexagesimal",
	    "range" : ["+90 00 00.0", "-90 00 00.0"],
	    "default" : "+00 00 00.0",
	    "description" : "Sexagesimal declination δ."
	},

	"alt" :  {
	    "type" : "float",
	    "range" : [20.0, 90.0],
	    "default" : 85.0,
	    "description" : "Altitude angle in degrees. Lower altitude is set to 20°."
	},

	"az" :  {
	    "type" : "float",
	    "range" : [0.0, 359.9],
	    "default" : 0.0,
	    "description" : "Azimuth angle in degrees, measured from North toward the West."
	},

	"epoch" :  {
	    "type" : "float",
	    "range" : [1950.0, 2050.0],
	    "default" : 2000.0,
	    "description" : "Standard conventional Epoch J."
	},

	"offset" :  {

	    "ra" : {
		"type" : "integer",
		"range" : [0,15],
		"default" : 0,
		"description" : "Offset in arcsec with respect to ra coordinates."
	    },

	    "dec" : {
		"type" : "integer",
		"range" : [0,15],
		"default" : 0,
		"description" : "Offset in arcsec with respect to dec coordinates."
	    },

	    "description": "Defines two objects, .ra and .dec, giving offsets in arcsec with respect to ra and dec coordinates."
	},

	"track" :  {
	    "type" : "bool",
	    "range" : [true, false],
	    "default" : true,
	    "description" : "Telescope tracking the target, (true) or not (false)."
	},

	"derotate" :  {
	    "type" : "bool",
	    "range" : [true, false],
	    "default" : true,
	    "description" : "Auto-tracking of the telescope derotator, (true) or not (false)."
	},

	"derotator" :  {
	    "type" : "float",
	    "range" : [0, 480.0],
	    "default" : 0,
	    "description" : "Derotator angle in degrees clockwise."
	},

	"focus" :  {
	    "type" : "integer",
	    "range" : [10000, 20000],
	    "default" : 12345,
	    "description" : "Secondary position in mm."
	},

	"description" : "Telescope description."
    },


    "camera" : {

	"status" : {
	    "type" : "bool",
	    "range" : [true, false],
	    "default" : false,
	    "description" : "Camera status, on (true) or off (false)."
	},

	"exptime" :   {
	    "type" : "float",
	    "range" : [0.0, 3600.0],
	    "default" : 0.0,
	    "description" : "Exposure time in seconds. 3600=1 hour."
	},

	"filter" :   {
	    "type" : "string",
	    "range" : ["U", "B", "V", "R", "I", "Free"],
	    "default" : "R",
	    "description" : "Observing filter (UBVRI and Free)."
	},

	"binning" :   {
	    "type" : "integer",
	    "range" : [1,4],
	    "default" : 2,
	    "description" : "Binning mode in pixels (1=1x1, 2=2x2, 3=3x3, 4=4x4)."
	},

	"temperature" :   {
	    "type" : "float",
	    "range" : [-20,20],
	    "default" : -20,
	    "description" : "CCD temperature in degrees °C."
	},

	"image" : {
	    "type" : "string",
	    "range" : ["dark", "light"],
	    "default" : "light",
	    "description" : "Image type"
	},

	"frame" :   {
	    "type" : "string",
	    "range" : ["full","custom"],
	    "default" : "binned",
	    "description" : "Frame type. If \"custom\", then \"crop\" parameters are read.  "
	},

	"crop" : {

	    "x0" : {
		"type" : "integer",
		"range" : [0,1023],
		"default" : 0,
		"description" : "Lower X coordinate of the crop rectangle."
	    },

	    "y0" : {
		"type" : "integer",
		"range" : [0,1023],
		"default" : 0,
		"description" : "Lower Y coordinate of the crop rectangle."
	    },

	    "xsize": {
		"type" : "integer",
		"range" : [0,1023],
		"default" : 1023,
		"description" : "X size of the crop rectangle."
	    },

	    "ysize" : {
		"type" : "integer",
		"range" : [0,1023],
		"default" : 1023,
		"description" : "Y size of the crop rectangle."
	    },

	    "description" : "Crop parameters of the Camera."

	},

	"description" : "Camera description."
    },


    "allsky" : {

	"status" : {
	    "type" : "bool",
	    "range" : [true, false],
	    "default" : false,
	    "description" : "AllSky status, on (true) or off (false)."
	},

	"exptime" :   {
	    "type" : "float",
	    "range" : [0.0, 60.0],
	    "default" : 0.01,
	    "description" : "Exposure time in seconds. 3600=1 hour."
	},

	"type" : {
	    "type" : "string",
	    "range" : ["dark", "light", "auto"],
	    "default" : "auto",
	    "description" : "Image type. If .mode is \"auto\", a light-dark frame is taken using a 320x240 frame (2x2 binning). "
	},

	"frame" :   {
	    "type" : "string",
	    "range" : ["full","crop","custom"],
	    "default" : "crop",
	    "description" : "Frame type. \"full\": 640x480, \"crop\": 512x480. Binned 320x240 mode only works is .type is \"auto\"."
	},

	"crop" : {

	    "x0" : {
		"type" : "integer",
		"range" : [0,640],
		"default" : 260,
		"description" : "Lower X coordinate of the crop rectangle."
	    },

	    "y0" : {
		"type" : "integer",
		"range" : [0,480],
		"default" : 180,
		"description" : "Lower Y coordinate of the crop rectangle."
	    },

	    "size": {
		"type" : "integer",
		"range" : [2,127],
		"default" : 127,
		"description" : "Size of the crop square."
	    },

	    "description" : "Crop parameters of the AllSky camera. Instruction are read if .type is \"dark\" or \"light\", and if .mode is \"custom\"."

	},

	    "description" : "AllSky camera description."
    },

    
    "gps" : {

	"status" : {
	    "type" : "bool",
	    "range" : [true, false],
	    "default" : false,
	    "description" : "GPS status, on (true) or off (false)."
	},

	"description" : "GPS description."
    },

    "description": "Observatory Dictionary. If no \"type\" is set, then another sub-dictionary is defined for that item. If more than two elements are defined for \"range\", than only the specified ranges are possible."

}
